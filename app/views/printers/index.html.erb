<h1>Listing printers</h1>

<% if @printers.any? %>
    ПУ
    <table class="table">
      <thead>
      <tr>
        <th>Устройство</th>
        <th>Модель</th>
        <th>ID отделения</th>
      </tr>
      </thead>
      <tbody>

      <% @printers.each do |pr| %>
          <tr>
            <td><%= pr.device %></td>
            <td> <%= pr.printer_model %> </td>
            <td> <%= pr.station_id %> </td>
            <td> <%= link_to "просмотр", station_printer_path(@station, pr) %></td>
            <td><%= link_to "редактировать", edit_station_printer_path(@station, pr) %></td>
            <td><%= link_to "удалить", '#' %></td>
          </tr>
          </tbody>
      <% end %>
      </table>
<% end %>
